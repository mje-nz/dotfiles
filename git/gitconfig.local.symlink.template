# This file is included by the main .gitconfig file, and is not versioned. It is
# generated from a template file, which *is* versioned.

[user]
        name = AUTHORNAME
        email = AUTHOREMAIL
[credential]
        helper = GIT_CREDENTIAL_HELPER

# This is in here so that it can be stubbed out in CI
[url "ssh://git@github.com/"]
    # Replace HTTPS GitHub URLs with the SSH equivalent
    insteadOf = https://github.com/
